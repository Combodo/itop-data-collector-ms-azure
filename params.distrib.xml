<?xml version="1.0" encoding="UTF-8"?><!-- Default values for parameters. Do NOT alter this file, use params.local.xml instead -->
<parameters>
	<!-- iTop Parameters -->
	<itop_url>http://myitop.com</itop_url>
	<itop_login>admin</itop_login>
	<itop_password>admin</itop_password>

	<!-- Microsoft links -->
	<microsoft_login_url>https://login.microsoftonline.com/</microsoft_login_url>
	<microsoft_auth_mode>/oauth2/token</microsoft_auth_mode>
	<microsoft_resource>https://management.azure.com/</microsoft_resource>

	<!-- Client's credentials -->
	<azure_clientid></azure_clientid>
	<azure_clientsecret></azure_clientsecret>
	<azure_tenantid></azure_tenantid>

	<!-- App Services -->
	<azureappservicejsoncollector>
		<enable>yes</enable>
		<azure_class>Site</azure_class>
		<azure_api_version>2021-02-01</azure_api_version>
		<jsonfile>data/AzureAppServices.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<azureid>id</azureid>
			<azureresourcegroup_id>azureresourcegroup_id</azureresourcegroup_id>
			<azuresubscription_id>azuresubscription_id</azuresubscription_id>
			<location_id>location</location_id>
			<name>name</name>
			<org_id>org_id</org_id>
			<status>state</status>
		</fields>
		<defaults>
			<org_id>Demo</org_id>
			<azureresourcegroup_id></azureresourcegroup_id>
			<azuresubscription_id></azuresubscription_id>
			<status>implementation</status>
		</defaults>
	</azureappservicejsoncollector>

	<!-- Load balancers -->
	<azureloadbalancerjsoncollector>
		<enable>yes</enable>
		<azure_class>LoadBalancer</azure_class>
		<azure_api_version>2021-03-01</azure_api_version>
		<jsonfile>data/AzureLoadBalancer.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<azureid>id</azureid>
			<azureresourcegroup_id>azureresourcegroup_id</azureresourcegroup_id>
			<azuresubscription_id>azuresubscription_id</azuresubscription_id>
			<location_id>location</location_id>
			<name>name</name>
			<org_id>org_id</org_id>
			<status>state</status>
		</fields>
		<defaults>
			<org_id>Demo</org_id>
			<azureresourcegroup_id></azureresourcegroup_id>
			<azuresubscription_id></azuresubscription_id>
			<status>implementation</status>
		</defaults>
	</azureloadbalancerjsoncollector>

	<!-- Locations -->
	<azurelocationjsoncollector>
		<enable>yes</enable>
		<azure_class>locations</azure_class>
		<azure_api_version>2021-04-01-preview</azure_api_version>
		<jsonfile>data/AzureLocation.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<code>code</code>
			<name>name</name>
			<org_id>org_id</org_id>
			<status>state</status>
		</fields>
		<defaults>
			<code></code>
			<org_id>Demo</org_id>
			<status>active</status>
		</defaults>
	</azurelocationjsoncollector>

	<!-- Resources group -->
	<azureresourcegroupjsoncollector>
		<enable>yes</enable>
		<azure_class>resourceGroups</azure_class>
		<azure_api_version>2020-09-01</azure_api_version>
		<jsonfile>data/AzureResourceGroup.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<azureid>id</azureid>
			<azuresubscription_id>azuresubscription_id</azuresubscription_id>
			<location_id>location</location_id>
			<name>name</name>
			<org_id>org_id</org_id>
			<status>state</status>
		</fields>
		<defaults>
			<azuresubscription_id></azuresubscription_id>
			<org_id>Demo</org_id>
			<status>implementation</status>
		</defaults>
	</azureresourcegroupjsoncollector>

	<!-- Subscriptions -->
	<azuresubscriptionjsoncollector>
		<enable>no</enable>
		<azure_class>subscriptions</azure_class>
		<azure_api_version>2020-01-01</azure_api_version>
		<jsonfile>data/AzureSubscription.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<azureid>id</azureid>
			<discover_objects>discover_objects</discover_objects>
			<name>displayName</name>
			<offer>subscriptionPolicies/quotaId</offer>
			<org_id>org_id</org_id>
			<provider_id>provider_id</provider_id>
			<subscriptionid>subscriptionId</subscriptionid>
			<status>state</status>
		</fields>
		<defaults>
			<org_id>Demo</org_id>
			<provider_id>Demo</provider_id>
			<status>implementation</status>
			<discover_objects>no</discover_objects>
		</defaults>
	</azuresubscriptionjsoncollector>

	<!-- VNETs -->
	<azurevnetjsoncollector>
		<enable>yes</enable>
		<azure_class>VNet</azure_class>
		<azure_api_version>2021-03-01</azure_api_version>
		<jsonfile>data/AzureVNet.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<azureid>id</azureid>
			<azureresourcegroup_id>azureresourcegroup_id</azureresourcegroup_id>
			<azuresubscription_id>azuresubscription_id</azuresubscription_id>
			<location_id>location</location_id>
			<name>name</name>
			<org_id>org_id</org_id>
			<status>state</status>
		</fields>
		<defaults>
			<org_id>Demo</org_id>
			<azureresourcegroup_id></azureresourcegroup_id>
			<azuresubscription_id></azuresubscription_id>
			<status>implementation</status>
		</defaults>
	</azurevnetjsoncollector>

	<!-- Virtual machines -->
	<azurevirtualmachinejsoncollector>
		<enable>yes</enable>
		<azure_class>VirtualMachine</azure_class>
		<azure_api_version>2021-11-01</azure_api_version>
		<jsonfile>data/AzureVirtualMachine.json</jsonfile>
		<path>value</path>
		<fields>
			<primary_key>id</primary_key>
			<azureid>id</azureid>
			<azureresourcegroup_id>azureresourcegroup_id</azureresourcegroup_id>
			<azuresubscription_id>azuresubscription_id</azuresubscription_id>
			<location_id>location</location_id>
			<name>name</name>
			<org_id>org_id</org_id>
			<status>state</status>
		</fields>
		<defaults>
			<org_id>Demo</org_id>
			<azureresourcegroup_id></azureresourcegroup_id>
			<azuresubscription_id></azuresubscription_id>
			<status>implementation</status>
		</defaults>
	</azurevirtualmachinejsoncollector>

	<json_placeholders>
		<!-- For compatibility with the version 1.1.x of the collector, define the data table names as following:
		<prefix></prefix>
		<persons_data_table>synchro_data_PersonAD</persons_data_table>
		<users_data_table></users_data_table>
		 -->
		<azure_tenantid_short>c3cf8f43d5cd</azure_tenantid_short>
		<azureappservice_synchro_name>Azure App Services Discovery</azureappservice_synchro_name>
		<azureloadbalancer_synchro_name>Azure Load Balancers Discovery</azureloadbalancer_synchro_name>
		<azurelocation_synchro_name>Azure Locations Discovery</azurelocation_synchro_name>
		<azureresourcegroup_synchro_name>Azure Resources Group Discovery</azureresourcegroup_synchro_name>
		<azuresubscription_synchro_name>Azure Subscriptions Discovery</azuresubscription_synchro_name>
		<azurevirtualmachine_synchro_name>Azure Virtual Machines Discovery</azurevirtualmachine_synchro_name>
		<azurevnet_synchro_name>Azure VNets Discovery</azurevnet_synchro_name>
	</json_placeholders>

	<!-- mapping tables -->
	<subscription_status_mapping type="array">
		<pattern>/Deleted/obsolete</pattern>
		<pattern>/Disabled/implementation</pattern>
		<pattern>/Enabled/production</pattern>
		<pattern>/PastDue/obsolete</pattern>
		<pattern>/Warned/implementation</pattern>
	</subscription_status_mapping>

</parameters>